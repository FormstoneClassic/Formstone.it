s current image values","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_nextImage","description":"Handles next button click","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_previousImage","description":"Handles previous button click","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_zoomIn","description":"Handles zoom in button click","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_zoomOut","description":"Handles zoom out button click","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_clearZoom","description":"Clears current zoom action","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_setZoomPosition","description":"Sets zoom position","params":[{"name":"data","type":"object","description":"Instance Data"},{"name":"left","type":"number","description":"Left position"},{"name":"top","type":"number","description":"Top position"}],"private":true},{"name":"_clearZoomPosition","description":"Clears zoom position","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_dragStart","description":"Handles drag start","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_onDrag","description":"Handles dragging","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_dragStop","description":"Handles drag end","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_onTouch","description":"Delegates touch event","params":[{"name":"e","type":"object","description":"Event Data"}],"private":true},{"name":"_onTouchStart","description":"Handles touch start","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_onTouchMove","description":"Handles touch move","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_onTouchEnd","description":"Handles touch end","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_normalizeSource","description":"Normalizes source string or object","params":[{"name":"data","type":"object","description":"Instance Data"}],"private":true},{"name":"_startAnimation","description":"Starts main animation loop","private":true},{"name":"_clearAnimation","description":"End main animation loop","private":true},{"name":"_onAnimate","description":"Handles RAF","private":true},{"name":"_prefix","description":"Builds vendor-prefixed styles","params":[{"name":"property","type":"string","description":"Property to prefix"},{"name":"value","type":"string","description":"Property value"}],"return":{"type":"string","description":"Vendor-prefixed style"},"private":true},{"name":"_getTransform3DSupport","description":"Determines if transforms are support","return":{"type":"boolean","description":"True if transforms supported"},"private":true}]},"files":{"js":["jquery.fs.zoomer.js"],"css":["jquery.fs.zoomer.css"]},"extra":"","demo_resources":"<!--[DEMO:START-RESOURCES]-->\r\n\r\n\t\t<script src=\"http:\/\/formstone.it\/components\/Zoetrope\/jquery.fs.zoetrope.min.js\"><\/script>\r\n\r\n\t\t<style>\r\n\t\t\t.demo .zoomer_wrapper { border: 1px solid #ddd; border-radius: 3px; height: 500px; margin: 10px 0; overflow: hidden; width: 100%; }\r\n\r\n\t\t\t.demo .zoomer.dark_zoomer { background: #333 url(http:\/\/formstone.it\/files\/demo\/zoomer-bg-dark.png) repeat center; }\r\n\t\t\t.demo .zoomer.dark_zoomer img { box-shadow: 0 0 5px rgba(0, 0, 0, 0.5); }\r\n\t\t<\/style>\r\n\r\n\t\t<script>\r\n\t\t\t$(document).ready(function() {\r\n\t\t\t\t$(\".demo .zoomer_basic\").zoomer();\r\n\r\n\t\t\t\t$(\".demo .zoomer_custom\").zoomer({\r\n\t\t\t\t\tcontrols: {\r\n\t\t\t\t\t\tzoomIn: \".zoomer_custom_zoom_in\",\r\n\t\t\t\t\t\tzoomOut: \".zoomer_custom_zoom_out\"\r\n\t\t\t\t\t},\r\n\t\t\t\t\tcustomClass: \"dark_zoomer\",\r\n\t\t\t\t\tincrement: 0.03,\r\n\t\t\t\t\tinterval: 0.1,\r\n\t\t\t\t\tmarginMax: 50\r\n\t\t\t\t});\r\n\r\n\t\t\t\t$(\".demo .load_zoomer_tiled\").one(\"click\", function() {\r\n\t\t\t\t\t$(this).after('<div class=\"zoomer_wrapper zoomer_tiled\"><\/div>').remove();\r\n\t\t\t\t\t$(\".demo .zoomer_tiled\").zoomer({\r\n\t\t\t\t\t\tsource:{\r\n\t\t\t\t\t\t\tthumbnail: \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_thumbnail.jpg\",\r\n\t\t\t\t\t\t\ttiles: [\r\n\t\t\t\t\t\t\t\t[\"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_01.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_02.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_03.jpg\", ],\r\n\t\t\t\t\t\t\t\t[\"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_04.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_05.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_06.jpg\", ],\r\n\t\t\t\t\t\t\t\t[\"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_07.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_08.jpg\", \"http:\/\/formstone.it\/files\/demo\/space-tiled\/tiled_09.jpg\", ],\r\n\t\t\t\t\t\t\t]\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t});\r\n\t\t\t\t});\r\n\r\n\t\t\t\t$(window).on(\"resize\", function(e) {\r\n\t\t\t\t\t$(\".demo .zoomer_wrapper\").zoomer(\"resize\");\r\n\t\t\t\t});\r\n\r\n\t\t\t\t$(\".demo_tabbed\").on(\"update.tabber\", function() {\r\n\t\t\t\t\t$(\".demo .zoomer_wrapper\").zoomer(\"resize\");\r\n\t\t\t\t});\r\n\r\n\t\t\t\t$(window).one(\"pronto.load\", function() {\r\n\t\t\t\t\t$(\".demo .zoomer_basic\").zoomer(\"destroy\");\r\n\t\t\t\t\t$(\".demo .zoomer_custom\").zoomer(\"destroy\");\r\n\t\t\t\t\t$(\".demo .zoomer_tiled\").zoomer(\"destroy\");\r\n\t\t\t\t\t$(\".demo .load_zoomer_tiled\").off(\"click\");\r\n\t\t\t\t\t$(\".demo_tabbed\").off(\"update.tabber\");\r\n\t\t\t\t});\r\n\t\t\t});\r\n\t\t<\/script>\r\n\r\n\t\t","demo_content":"<!--[DEMO:START-CONTENT]-->\r\n\r\n\t\t\t\t<!-- Basic -->\r\n\t\t\t\t<h2>Basic<\/h2>\r\n\t\t\t\t<p>Create a new Zoomer by simply including an image inside the target wrapper. The target wrapper should have a predefined height and width as the new markup will fill all available space:<\/p>\r\n\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-javascript\">$(\".viewer\").zoomer();<\/code><\/pre>\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-css\">.viewer { height: 500px; width: 500px; }<\/code><\/pre>\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-markup\">&lt;div class=\"viewer\"&gt;\r\n    &lt;img src=\"\/path\/to\/image.jpg\"&gt;\r\n&lt;\/div&gt;<\/code><\/pre>\r\n\r\n\t\t\t\t<h5>Demo<\/h5>\r\n\t\t\t\t<div class=\"zoomer_wrapper zoomer_basic\">\r\n\t\t\t\t\t<img src=\"http:\/\/formstone.it\/files\/demo\/nasa-bw-1.jpg\">\r\n\t\t\t\t<\/div>\r\n\r\n\t\t\t\t<!-- GALLERY -->\r\n\t\t\t\t<h2>Gallery<\/h2>\r\n\t\t\t\t<p>Zoomer will auto detect multiple image and build a gallery:<\/p>\r\n\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-javascript\">$(\".viewer\").zoomer();<\/code><\/pre>\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-markup\">&lt;div class=\"viewer\"&gt;\r\n    &lt;img src=\"\/path\/to\/image-1.jpg\"&gt;\r\n    &lt;img src=\"\/path\/to\/image-2.jpg\"&gt;\r\n    &lt;img src=\"\/path\/to\/image-3.jpg\"&gt;\r\n&lt;\/div&gt;<\/code><\/pre>\r\n\r\n\t\t\t\t<h5>Demo<\/h5>\r\n\t\t\t\t<div class=\"zoomer_wrapper zoomer_basic\">\r\n\t\t\t\t\t<img src=\"http:\/\/formstone.it\/files\/demo\/nasa-bw-2.jpg\">\r\n\t\t\t\t\t<img src=\"http:\/\/formstone.it\/files\/demo\/nasa-bw-1.jpg\">\r\n\t\t\t\t\t<img src=\"http:\/\/formstone.it\/files\/demo\/nasa-bw-3.jpg\">\r\n\t\t\t\t<\/div>\r\n\r\n\t\t\t\t<br>\r\n\r\n\t\t\t\t<!-- ADVANCED -->\r\n\t\t\t\t<h2>Advanced<\/h2>\r\n\t\t\t\t<p>Customize a specific Zoomer instance:<\/p>\r\n\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-javascript\">$(\".viewer\").zoomer({\r\n\tcontrols: {\r\n\t\tzoomIn: \".zoom_in\",\r\n\t\tzoomOut: \".zoom_out\"\r\n\t}\r\n\tcustomClass: \"custom\",\r\n\tincrement: 0.03,\r\n\tinterval: 0.1,\r\n\tmarginMax: 50\r\n});<\/code><\/pre>\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-markup\">&lt;div class=\"viewer\"&gt;\r\n    &lt;img src=\"\/path\/to\/image.jpg\"&gt;\r\n&lt;\/div&gt;<\/code><\/pre>\r\n\r\n\t\t\t\t<h5>Demo<\/h5>\r\n\t\t\t\t<div class=\"zoomer_wrapper zoomer_custom\">\r\n\t\t\t\t  <img src=\"http:\/\/formstone.it\/files\/demo\/space.jpg\">\r\n\t\t\t\t<\/div>\r\n\t\t\t\t<div class=\"clear_fix\">\r\n\t\t\t\t\t<span class=\"button small inline zoomer_custom_zoom_in\">In<\/span>\r\n\t\t\t\t\t<span class=\"button small inline zoomer_custom_zoom_out\">Out<\/span>\r\n\t\t\t\t<\/div>\r\n\r\n\t\t\t\t<br>\r\n\r\n\t\t\t\t<!-- TILED -->\r\n\t\t\t\t<h2>Tiled<\/h2>\r\n\t\t\t\t<p>Zoomer can also handle larger images that have been sliced into equal tiles. You must pass the tiles at initialization:<\/p>\r\n\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-javascript\">$(\".viewer\").zoomer({\r\n    source: {\r\n        thumbnail: \"images\/thumbnail.jpg\",\r\n        tiles: [\r\n\t\t\t[\"images\/01-01.jpg\", \"images\/01-02.jpg\", \"images\/01-03.jpg\"],\r\n            [\"images\/02-01.jpg\", \"images\/02-02.jpg\", \"images\/02-03.jpg\"],\r\n            [\"images\/03-01.jpg\", \"images\/03-02.jpg\", \"images\/03-03.jpg\"]\r\n        ]\r\n    }\r\n});<\/code><\/pre>\r\n\t\t\t\t<pre class=\"example\"><code class=\"language-markup\">&lt;div class=\"viewer\"&gt;\r\n    &lt;-- Remains Empty --\/&gt;\r\n&lt;\/div&gt;<\/code><\/pre>\r\n\r\n\t\t\t\t<h5>Demo<\/h5>\r\n\t\t\t\t<div class=\"clear_fix\">\r\n\t\t\t\t\t<span class=\"button small inline load_zoomer_tiled\">Load Demo<\/span>\r\n\t\t\t\t<\/div>\r\n\r\n\t\t\t\t"}